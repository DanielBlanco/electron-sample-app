{
  "name": "electron-sample-app",
  "version": "1.0.0",
  "description": "Just playing with electron",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "test": "echo \"Error: no test specified\" && exit 1",
    "package-mac": "electron-packager . --overwrite --platform=darwin --arch=x64 --icon=assets/icons/mac/icon.icns --prune=true --out=release-builds",
    "package-win": "electron-packager . electron-tutorial-app --overwrite --asar=true --platform=win32 --arch=ia32 --icon=assets/icons/win/icon.ico --prune=true --out=release-builds --version-string.CompanyName=CE --version-string.FileDescription=CE --version-string.ProductName=\"Shopping List\"",
    "package-linux": "electron-packager . electron-tutorial-app --overwrite --asar=true --platform=linux --arch=x64 --icon=assets/icons/png/icon.png --prune=true --out=release-builds"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/DanielBlanco/electron-sample-app.git"
  },
  "keywords": [
    "electron"
  ],
  "author": "dblancorojas",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/DanielBlanco/electron-sample-app/issues"
  },
  "homepage": "https://github.com/DanielBlanco/electron-sample-app#readme",
  "devDependencies": {
    "electron": "^2.0.0",
    "electron-packager": "^12.0.2"
  }
}
